        .skill-card:hover {
            transform: translateY(-8px);
            box-shadow: 0 15px 40px rgba(0,0,0,0.15);
        }

        .skill-card h3 {
            color: #2980b9;
            margin-bottom: 1.2rem;
            font-size: 1.5rem;
        }

