            background: linear-gradient(135deg, #2c3e50 0%, #3498db 100%);
            background-attachment: fixed;
            margin: 0;
            padding: 0;
        }

        .container {
            width: 90%;
            max-width: 1200px;
            margin: 0 auto;
            padding: 20px;
        }

        header {
            background: linear-gradient(135deg, #8e44ad 0%, #2980b9 100%);
