            display: inline-block;
            padding: 0.4rem 1rem;
            border-radius: 20px;
            font-weight: bold;
            margin: 0.5rem 0;
        }

        .real-skill {
            background: #27ae60;
            color: white;
        }

        .fictional-skill {
            background: #e74c3c;
            color: white;
        }

        .example-box {
            background: #e8f4fd;
            padding: 1.2rem;
            border-radius: 8px;
            margin: 1.2rem 0;
            border-left: 4px solid #3498db;
        }

        .example-title {
            font-weight: bold;
            color: #2980b9;
            margin-bottom: 0.5rem;
        }

        .example-command {
            font-family: 'Courier New', monospace;
            background: #2c3e50;
            color: #ecf0f1;
            padding: 0.8rem;
            border-radius: 4px;
            white-space: pre-wrap;
            overflow-x: auto;
        }

        .architecture-section {
            background: #ecf0f1;
        }

        .architecture-diagram {
            display: grid;
            grid-template-columns: 1fr 2fr 1fr;
            gap: 2rem;
            margin: 2rem 0;
            text-align: center;
